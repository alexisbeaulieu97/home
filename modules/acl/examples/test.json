{
    "version": "1.0",
    "rules": [
        {
            "id": "dry-run-test",
            "roots": [
                "/tmp"
            ],
            "include_root": true,
            "depth": "infinite",
            "match": {
                "types": [
                    "directory"
                ],
                "pattern_syntax": "glob",
                "include": [
                    "*"
                ]
            },
            "acl": {
                "directories": [
                    "m::r-x"
                ]
            }
        }
    ]
}